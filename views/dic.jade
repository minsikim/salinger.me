doctype html
html
  head
    meta(charset="utf-8", name="viewport", content="width=device-width, initial-scale=1")
    title Salinger MS
    link(rel='stylesheet', href='css/style.css')
    link(rel="stylesheet", href="//fonts.googleapis.com/css?family=Lato:100,200,300,400,500,600,700,800,900")
    link(rel="stylesheet", href="//fonts.googleapis.com/css?family=Nothing+You+Could+Do")
    
  body(id='dic', style="background-image: url(\"/img/backgrounds/blue.jpg\");")
    .wrapper
      .head-row
        .fix-nav
          .menubar
            .menu-icon
            .menu-icon
            .menu-icon
          .titlebar
            span.salinger Salinger
            span.ms  MS
        div(style="height:60px;")
        input.search(type="text", value='search')
        .main-title-div
          span.main-title My Dictionary
      .content-wrapper
        .dic-wrapper
          .dic-box
            .dic-word
              .word .html()
              .tag-group
                .tag javascript
                .tag jquery
            .desc Get the HTML contents of the first element in the set of matched<br>elements or set the HTML contents of every matched element.
          .dic-box
            .dic-word
              .word .$()
              .tag-group
                .tag javascript
                .tag jquery
            .desc Get the HTML contents of the first element in the set of<br>matched elements or set the HTML contents of every matched element.
          .dic-box
            .dic-word
              .word .attr()
              .tag-group
                .tag javascript
                .tag jquery
            .desc Get the HTML contents of the first element in the set of matched elements<br>or set the HTML contents of every matched element.
